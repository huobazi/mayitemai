<ul class="breadcrumb"><li><a href="/site/welcome">首页</a></li><li><a href="/myContract/list">我的合同</a></li><li class="active">创建合同</li></ul>

<div class="alert alert-warning">提示：标注 <span class="required">*</span> 为必添项</div>

<table class="table table-striped table-bordered table-condensed table-hover">

  <% if @brand.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@brand.errors.count, "error") %> prohibited this brand from being saved:</h2>
      <ul>
        <% @brand.errors.full_messages.each do |message| %>
          <li><%= message %></li>
        <% end %>
      </ul>
    </div>
  <% end %>
  <%= form_for(@brand, html: {class: "form-horizontal"}) do |f| %>

    <tr>
      <td width="15%">
        <label class="required">
          专题名称
          <span class="required">*</span>
        </label>
      </td>
      <td colspan="3">
        <div class="col-sm-4">
          <%= f.text_field :name%>
          <input type="text" class="form-control">
        </div>
      </td>
    </tr>

    <tr>
      <td width="15%">
        <label class="required">
          店铺旺旺
          <span class="required">*</span>
        </label>
      </td>
      <td colspan="3">
        <div class="col-sm-4">
          <input type="text" class="form-control">
        </div>
      </td>
    </tr>

    <tr>
      <td width="15%">
        <label class="required">
          头部图片
          <span class="required">*</span>
        </label>
      </td>
      <td colspan="3">
        <div class="col-sm-4">
          <input type="text" class="form-control">
        </div>
      </td>
    </tr>

    <tr>
      <td width="15%">
        <label class="required">
          相关推荐
          <span class="required">*</span>
        </label>
      </td>
      <td colspan="3">
        <div class="col-sm-4">
          <input type="text" class="form-control">
        </div>
      </td>
    </tr>

    <tr>
      <td width="15%">
        <label class="required">
          状态
          <span class="required">*</span>
        </label>
      </td>
      <td colspan="3">
        <div class="col-sm-4">
          <input type="text" class="form-control">
        </div>
      </td>
    </tr>
  </tr>

</table>
<div class="form-actions">
  <button class="btn btn-primary" id="yw0" type="submit" name="yt0">创建</button></div>
<tr>
<% end %>
