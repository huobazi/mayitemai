<ul class="breadcrumb">
  <li>
    <a href="/">首页</a>
  </li>
  <li>
    <a href="<%= goods_path %>">商品管理</a>
  </li>
  <li class="active">
    创建商品
  </li>
</ul>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @goods.each do |good| %>
      <tr>
        <td><%= link_to 'Show', good %></td>
        <td><%= link_to 'Edit', edit_good_path(good) %></td>
        <td><%= link_to 'Destroy', good, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Good', new_good_path %>
